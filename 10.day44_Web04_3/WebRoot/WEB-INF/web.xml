<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>10.day43_Web03</display-name>
  




  <servlet>
    <servlet-name>Action</servlet-name>
    <servlet-class>web.ActionServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Some</servlet-name>
    <servlet-class>web.SomeServlet</servlet-class>
    <init-param>
    	<param-name>company</param-name>
    	<param-value>Guangzhou_Patrick_Inc</param-value>
    </init-param>
    <!-- tell servlet create this servlet in advance -->
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  
 


  <servlet-mapping>
    <servlet-name>Action</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Some</servlet-name>
    <url-pattern>/some</url-pattern>
  </servlet-mapping>
</web-app>